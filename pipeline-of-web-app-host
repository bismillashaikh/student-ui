pipeline {
    agent {
        node {
            label 'student'
        }

    }
    stages {
        stage('git commit') {
            steps {

                sh 'https://github.com/bismillashaikh/student-ui.git'
                echo "successfull git commit stage completed"
            }
        }
        stage('build') {
            steps {
                sh '''
                echo "successfully build"
                '''
            }
        }
        stage('test') {
            steps {
                echo "successfully test"
            }
        }
        stage('deploy') {
            steps {
              echo "successfully deployed"
            }
        }
    
    }
}
